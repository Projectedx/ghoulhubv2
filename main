TextButton_12.MouseButton1Down:connect(function()
		_G.on = 0
		while _G.on == 1
		do
			wait(0)
			local Remote = game.ReplicatedStorage['RemoteEvent']

			local Arguments = {
				[1] = {
					[1] = "Skill_SpherePunch",
					[2] = Vector3.new(-808.782837,250.059631,749.671082)
				}
			}

			Remote:FireServer(unpack(Arguments))
			local Remote = game.ReplicatedStorage['RemoteEvent']

			local Arguments = {
				[1] = {
					[1] = "Skill_BulletPunch",
					[2] = "Right",
					[3] = Vector3.new(-707.280029,246.219849,751.109802)
				}
			}

			Remote:FireServer(unpack(Arguments))
			wait()
			local Remote = game.ReplicatedStorage['RemoteEvent']

			local Arguments = {
				[1] = {
					[1] = "Skill_BulletPunch",
					[2] = "Left",
					[3] = Vector3.new(-707.280029,246.219849,751.109802)
				}
			}

			Remote:FireServer(unpack(Arguments))
		end
end)

