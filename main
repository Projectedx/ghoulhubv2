	while wait do
		wait (.1) -- how fast to switch between ranks
		local Remote = game.ReplicatedStorage['RemoteEvent']

		local Arguments = {
			[1] = {
				[1] = "ChangeRankEmblem",
				[2] = math.random (1,10) --1 = min rank 12 = max rank
			}
		}

		Remote:FireServer(unpack(Arguments))
	end
end)


